{
    "name": "arueckauer/minion",
    "type": "project",
    "description": "Minimal CLI app with config and DI",
    "homepage": "https://github.com/arueckauer/minion",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Andi RÃ¼ckauer",
            "email": "arueckauer@gmail.com",
            "homepage": "https://arueckauer.github.io/",
            "role": "Project lead"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "laminas/laminas-config-aggregator": "^1.2",
        "laminas/laminas-servicemanager": "^3.4",
        "symfony/console": "^5.1"
    },
    "require-dev": {
        "laminas/laminas-coding-standard": "^2.0",
        "phpunit/phpunit": "^9.2",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Minion\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MinionTest\\": "test/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
